{
  "name": "artis",
  "version": "2.0.0",
  "description": "Modern Cesium wrapper engine with improved architecture",
  "type": "module",
  "main": "dist/artis.umd.js",
  "module": "dist/artis.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docs": "typedoc --out docs src/index.ts"
  },
  "keywords": [
    "cesium",
    "gis",
    "3d",
    "map",
    "webgl"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "prettier": "^3.2.5",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "cesium": "^1.127.0"
  },
  "peerDependencies": {
    "cesium": ">=1.118.0"
  }
}